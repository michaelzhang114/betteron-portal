<!DOCTYPE html>
<html>
  <head>
    <title>BetterOnPortal</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-12">
          <%= render 'shared/nav' %>
        </div>
      </div>

    </div>

    <div class="container">
      <div class="row">
        <div class="col-md-12">

          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
